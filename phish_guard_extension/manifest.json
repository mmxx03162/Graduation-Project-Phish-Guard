{
    "manifest_version": 3,
    "name": "Phish-Guard",
    "description": "An AI-powered extension to detect phishing websites in real-time.",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "host_permissions": [
      "http://127.0.0.1:8000/*"
    ],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }